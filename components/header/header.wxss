/* components/header/header.wxss */
.title {  
  width: 100%;  
  background-color: #fff;  
  box-sizing: border-box;  
  transform: translateZ(0);  
  z-index: 999990;
  display: fixed;
  top: 0rpx;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.title-text {  
  font-size: 34rpx;  
  color: #333333;  
  line-height: 44px;
  margin-left: 34rpx;
  text-align: center
  }
.back-icon{
  height:30rpx;
  width: 32rpx;
  margin-left: 30rpx;
}

.horizontal-line{
  background: #eeeeee;
  width: 100%;
  height: 1rpx;
}

