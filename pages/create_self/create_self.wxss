/**index.wxss**/

page {
    background-color: #fafafa;
}

.self-container {}

.top-tab-wrap {
    position: fixed;
    width: 100%;
    z-index: 2;
    background-color: #ffffff;
    top: 0;
    left: 0;
}

.tab1-content {
    display: flex;
    padding-top: 100rpx;
}

.hangye-list {
    width: 240rpx;
    background-color: #fafafa;
    text-align: center;
    color: #999999;
    font-size: 28rpx;
    max-height: 988rpx;
    overflow: auto;
}

.hangye-list .list-item {
    height: 100rpx;
    line-height: 100rpx;
}

.hangye-list .list-item.selected .item-txt {
    width: 100%;
    display: inline-block;
    width: 98%;
    border-right: 5rpx solid #5398ff;
    color: #333333;
    line-height: 1.4;
}

.xingwei-list {
    color: #666666;
    font-size: 30rpx;
    flex: 1 1 auto;
    max-height: 988rpx;
    overflow: auto;
    background-color: #ffffff;
}

.xingwei-list .list-item {
    height: 100rpx;
    line-height: 100rpx;
    width: 100%;
    display: flex;
}

.xingwei-list .list-item label {
    flex: 1 1 auto;
    padding-left: 40rpx;
}

.xingwei-list .list-item .item-score {
    font-size: 26rpx;
    color: #999999;
    margin-left: 6rpx;
}

checkbox .wx-checkbox-input {
    width: 40rpx;
    height: 40rpx;
    border: none;
    margin-top: -6rpx;
    margin-left: 185rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    background-image: url('data:image/png;base64,');
    background-size: 38rpx 38rpx;
    background-position: center;
    background-repeat: no-repeat;
    color: transparent;
}

.bottom-btns-area {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 120rpx;
    box-shadow: 0px -4px 10px 0px rgba(204, 204, 204, 0.2);
    display: flex;
    align-items: center;
    padding: 0 30rpx;
    background-color: #ffffff;
}

.bottom-btns-area .num-counts {
    font-size: 30rpx;
    color: #333333;
}

.the-num {
    color: #5398ff;
}

.reset-btn {
    font-size: 30rpx;
    color: #5398ff;
    margin-left: 170rpx;
}

.sure-btn {
    color: #ffffff;
    font-size: 30rpx;
    width: 160rpx;
    height: 80rpx;
    background-color: #5398ff;
    border-radius: 4rpx;
    border: none;
}

.zidingyi-area {
    width: 702rpx;
    height: 120rpx;
    background-color: #ffffff;
    border-radius: 4rpx;
    border: solid 1px #e5e5e5;
    margin: 120rpx auto 0;
    font-size: 30rpx;
    display: flex;
    align-items: center;
    padding: 0 26rpx;
    box-sizing: border-box;
}

.add-item-btn {
    width: 40rpx;
    height: 40rpx;
    background-image: url('data:image/png;base64,');
    background-position: center;
    background-size: contain;
    background-repeat: no-repeat;
}

.xingwei-name {
    color: #666666;
    width: 430rpx;
    margin: 0 30rpx 0 16rpx;
    height: 40rpx;
    border-right: 1px solid #e6e6e6;
}

.set-scores {
    font-size: 30rpx;
    color: #5398ff;
}

.top-tips {
    color: #cccccc;
    font-size: 24rpx;
}

.lists-wrap {
    padding: 0 25rpx;
}

.zidingyi-list {
    border-top: solid 1px #eeeeee;
    background-color: #ffffff;
    margin-bottom: 120rpx;
}

.zidingyi-list .list-item {
    display: flex;
    align-items: center;
    padding: 0 30rpx;
    height: 120rpx;
    border: solid 1px #eeeeee;
    border-top: none;
}

.zidingyi-list .list-item .remove-item-btn {
    width: 40rpx;
    height: 40rpx;
    background-image: url('data:image/png;base64,');
    background-position: center;
    background-size: contain;
    background-repeat: no-repeat;
    margin-right: 20rpx;
}

.zidingyi-list .list-item .item-score {
    font-size: 32rpx;
    color: #999999;
    flex: 1 1 auto;
    text-align: right;
}

.zidingyi-list .list-item .item-txt {
    color: #333333;
    font-size: 32rpx;
}

.select-score-modal {
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 9;
}

.modal-score-wrap {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
}

.modal-score-item {
    font-size: 32rpx;
    background-color: #ffffff;
    color: #333333;
    text-align: center;
    height: 120rpx;
    line-height: 120rpx;
    border-bottom: #ededed 1px solid;
    opacity: 1;
}