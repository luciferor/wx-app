<view>
    <view class="topbox">
        <view class="searchbox">
            <span class="fl inputimg">
                <image src="../../images/icon_sousuo.png" />
            </span>
            <span class="fr inputtxt">
                <input placeholder="搜索对象" auto-focus/>
            </span>
        </view>
    </view>
    <view class="i-index-demo">
        <i-index height="100%" bind:change="onChange">
            <i-index-item 
                wx:for="{{ cities }}" 
                wx:for-index="index" 
                wx:key="{{index}}" 
                wx:for-item="item" name="{{item.name}}">
                <view 
                    class="i-index-demo-item" 
                    wx:for="{{item.list}}" 
                    wx:for-index="in" 
                    wx:key="{{in}}" 
                    wx:for-item="it">
                    {{it.name}}
                </view>
            </i-index-item>
        </i-index>
    </view>
</view>