page{
  width: 100%;
  height: 100%;
  background: #f7f7f7;
}

.layout-container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.content-container{
  background: white;
  margin: 20rpx 24rpx 0px 24rpx;
}

.cell{
  padding: 35rpx 30rpx ;
  color: #333333;
  font-size: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 1rpx;
  }

.title{
  padding: 35rpx 30rpx ;
  font-size: 28rpx;
  color: #666666;
}

.bottom-container{
  background: white;
  padding: 20rpx 0rpx;
  width: 100%;
}

.button{
  background: #5398ff;
  margin-left: 24rpx;
  margin-right: 24rpx;
  border-radius: 4rpx;
  height: 110rpx;
  line-height: 110rpx;
  font-size: 36rpx;
  color: white;
  text-align: center;
}

.arrow{
  width: 30rpx;
  height: 30rpx;
}

.horizontal-line{
  background: #eeeeee;
  width: 100%;
  height: 1rpx;
}


