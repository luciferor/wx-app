.fl{
    float: left;
}
.fr{
    float: right;
}

.tab-h{
    height: 100rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 100rpx;
    background:white;
    font-size: 33rpx;
    white-space: nowrap;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 0;
    text-align: center;
}

.tab-item{
    margin:0 36rpx;
    color:#999999;
    display: inline-block;
    font-size: 32rpx;
}
.tab-item.active{
    font-size: 35rpx;
    color:#333333;
    position: relative;
}
.tab-item.active:after{
    content: "";
    display: block;
    height: 8rpx;
    width: 62rpx;
    background: #4675F9;
    position: absolute;
    bottom:15rpx;
    left:39rpx;
    border-radius: 16rpx;
}
.item-ans{ 
    width: 100%;
    display: flex;
    flex-grow: row no-wrap;
    justify-content: space-between;
    padding:30rpx;
    box-sizing: border-box;
    align-items: center;
    border-bottom: 1px solid #F2F2F2;
    background:white;
    flex-direction: row;
}
.item-left{
    display: flex;
    flex-grow: row no-wrap;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}
.tab-content{
    margin-top:100rpx;
    padding:24rpx;
    background:#f7f7f7;
}
