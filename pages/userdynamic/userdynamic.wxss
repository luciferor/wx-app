.fl{
    float: left;
}
.fr{
    float: right;
}

.user-list-content{
    width:calc(100% - 60rpx);
    border-bottom: 1px solid #eeeeee;
    height: 120rpx;
    line-height: 120rpx;
    padding:30rpx;
}

.under-list-img-box{
    width:100rpx;
    height: 100rpx;
    padding:10rpx;
}

.under-list-img-box image{
    width:98rpx;
    height: 98rpx;
    border-radius:69px;
	border: solid 1px #5398ff;
}

.user-li-title-box{
    height: 120rpx;
    padding-left:30rpx;
}

.user-li-title-box-listtitle{
    height: 60rpx;
    line-height: 60rpx;
}

.user-li-title-box-listtitle-name{
    font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #666666;
}
.user-li-title-box-listtitle-time{
    font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
    color: #999999;
    padding-left:20rpx;
}

.user-li-title-box-listdes{
    height: 60rpx;
    line-height: 60rpx;
    font-size: 32rpx;
    color: #333333;
}

.user-li-right-bf{
  font-size: 28rpx;
	color: #5398ff;
}