.fl{
  float: left;
}

.fr{
  float:right;
}

.container {
  height:100% !important;
  min-height:100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding:0 !important;
}

.title {
  width: 70%;
  height: 100rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.titleSel {
  color: #333333;
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.titleUnsel {
  color: #858fab;
  font-size: #858fab;
}

.headerLineSel {
  width: 68rpx;
	height: 6rpx;
	background-color: #6680ff;
	border-radius: 3rpx;
  margin-top: 10rpx;
}

.headerLineUnsel {
  background: #fff;
  width: 68rpx;
	height: 6rpx;
	border-radius: 3rpx;
  position: relative;
  margin-top: 10rpx;
}

.swiper {
  width:100%;
  flex:1;
  overflow:scroll;
  padding:48rpx;
  background:#f7f7f7;
  height:calc(100% -100rpx) !important;
}

.list-box{
  background:white;
  overflow: hidden;
  margin:0rpx 48rpx;
  border:1px solid #eeeeee;
}

.listcontent{
  height:100rpx;
  border-bottom: 1px solid #eeeeee;
  line-height: 100rpx;
  padding:20rpx;
}

