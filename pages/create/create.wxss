/**create.wxss**/

.create-container {
    height: 100%;
    display: flex;
    background-color: #f7f7f7;
    margin-bottom: 250rpx;
    flex-direction: column;
}

.setting-area {
    width: 700rpx;
    margin: 20rpx auto 0;
    font-size: 30rpx;
}

.setting-item {
    height: 100rpx;
    line-height: 100rpx;
    background-color: #fff;
    border: solid 1px #eee;
    padding: 0 22rpx 0 34rpx;
    border-bottom: none;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.setting-item.organize-name {
    display: block;
}

.setting-item .left-txt {
    color: #333333;
    font-size: 30rpx;
}

.setting-item .left-txt .txt-tips {
    color: #cccccc;
    font-size: 28rpx;
    margin-left: 18rpx;
}

.setting-item .right-arrow {
    width: 40rpx;
    height: 40rpx;
}

.rules-state-wrap {
    margin: 20rpx 0 0 0;
    border-radius: 4px;
    border: solid 1px #eeeeee;
    background-color: #fff;
    padding: 0 20rpx 0 20rpx;
}

.rules-state-wrap.expend {
    padding-bottom: 20rpx;
}

.rules-state {
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 4rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.rules-content {
    font-size: 24rpx;
    color: #999999;
    line-height: 50rpx;
    padding-bottom: 20rpx;
    box-sizing: content-box;
}

.rules-state .left-tips {
    padding-left: 20rpx;
    border-left: 6rpx solid #5398ff;
    height: 30rpx;
    line-height: 30rpx;
}

.rules-state .toggle-btn {
    width: 35px;
    height: 20px;
    background-image: url('data:image/png;base64,');
    background-size: contain;
    background-position: center;
    background-repeat: no-repeat;
}

.rules-state .toggle-btn.showing {
    background-image: url('data:image/png;base64,');
}

.bottom-operation {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 250rpx;
    background-color: #fff;
    border-top: 1px solid #eee;
}

.agreement-reading {
    display: flex;
    align-items: center;
    font-size: 30rpx;
    margin-top: 10rpx;
}

.to-read {
    display: inline-block;
    color: #5398ff;
}

.create-and-invite {
    background-color: #5398ff;
    width: 700rpx;
    height: 110rpx;
    line-height: 110rpx;
    color: #fff;
    margin: 15rpx auto 0;
    font-size: 36rpx;
}